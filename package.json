{
  "name": "homebrew-yard",
  "version": "0.1.0",
  "description": "Collection of Homebrew formulae",
  "private": true,
  "scripts": {
    "lint": "run-p format:check lint-es",
    "lint:fix": "run-s format lint-es:fix",
    "lint-es": "eslint --ext .ts,.tsx,.js,.jsx,.json .",
    "lint-es:file": "eslint --ext .ts,.tsx,.js,.jsx,.json",
    "lint-es:fix": "eslint --ext .ts,.tsx,.js,.jsx,.json --fix .",
    "lint-es:file:fix": "eslint --ext .ts,.tsx,.js,.jsx,.json --fix",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,vue,ts,css,less,scss,html,htm,json,md,markdown,yml,yaml}\"",
    "format:check": "prettier --ignore-path .eslintignore --check \"**/*.{js,jsx,vue,ts,css,less,scss,html,htm,json,md,markdown,yml,yaml}\"",
    "format:file": "prettier --write",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:clean-first": "run-s clean build",
    "clean": "rm -rf ./dist",
    "test": "jest",
    "test:ci": "jest --ci --verbose",
    "test:staged": "jest --passWithNoTests --findRelatedTests $(git diff --staged --name-only)",
    "update-workflow": "node ./dist/src/update-workflow",
    "formula:bottle": "node ./dist/src/formula/bottle",
    "formula:upload": "node ./dist/src/formula/upload",
    "formulae:create": "node ./dist/src/formulae/create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidavidorra/homebrew-yard.git"
  },
  "author": "Jeroen de Bruijn",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/vidavidorra/homebrew-yard/issues"
  },
  "homepage": "https://github.com/vidavidorra/homebrew-yard#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@jest/globals": "26.6.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/convict": "5.2.1",
    "@types/inquirer": "7.3.1",
    "@types/js-yaml": "3.12.5",
    "@types/mustache": "4.0.1",
    "@types/node": "14.14.6",
    "@types/yargs": "15.0.13",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "@vidavidorra/commitlint-config": "1.0.21",
    "ajv": "6.12.6",
    "alphanum-sort": "1.0.2",
    "aws-sdk": "2.785.0",
    "chalk": "4.1.0",
    "convict": "6.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "inquirer": "7.3.3",
    "jest": "26.6.3",
    "joi": "17.3.0",
    "js-yaml": "3.14.0",
    "json-stringify-pretty-compact": "2.0.0",
    "lint-staged": "10.5.1",
    "mustache": "4.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "request": "2.88.2",
    "semantic-release": "17.2.2",
    "shelljs": "0.8.4",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5",
    "yargs": "16.1.0"
  }
}
