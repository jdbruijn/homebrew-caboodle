{
  "name": "homebrew-collection",
  "version": "0.1.0",
  "description": "Collection of Homebrew formulae",
  "main": "index.js",
  "scripts": {
    "release": "standard-version --skip.commit --skip.tag",
    "release:ok": "standard-version --sign --skip.bump --skip.changelog",
    "release:publish": "git push --follow-tags",
    "create-formula": "node ./lib/create-formula"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidavidorra/homebrew-collection.git"
  },
  "author": "Jeroen de Bruijn",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/vidavidorra/homebrew-collection/issues"
  },
  "homepage": "https://github.com/vidavidorra/homebrew-collection#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "if [[ \"$(git rev-parse --abbrev-ref HEAD)\" != \"master\" ]]; then exit -1; fi",
      "postcommit": "git add package.json package-lock.json"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/download": "^6.2.4",
    "@types/inquirer": "0.0.44",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.11.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "chalk": "^2.4.2",
    "download": "^7.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "mustache": "^3.0.1",
    "prettier": "1.16.4",
    "standard-version": "^5.0.2"
  },
  "dependencies": {
    "typescript": "^3.3.3333"
  }
}
